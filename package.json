{
  "name": "learn-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "eslint src/** --ext .ts,.tsx",
    "lint:fix": "npm lint --fix",
    "prettier:quick": "pretty-quick --staged",
    "start": "next start",
    "clean": "if [ -d .serverless ]; then rm -rf .serverless ; fi; if [ -d .serverless_nextjs ]; then rm -rf .serverless_nextjs ; fi",
    "deploy": "yarn run clean && npx serverless --stage=development",
    "deploy:prod": "yarn run clean && npx serverless --stage=production"
  },
  "devDependencies": {
    "@serverless/aws-cloudfront": "^6.0.0",
    "@serverless/aws-lambda": "^3.0.3",
    "@serverless/aws-s3": "^4.2.0",
    "@serverless/domain": "^6.1.0",
    "@types/highlight.js": "^10.1.0",
    "@types/marked": "^1.2.0",
    "@types/node": "^14.14.9",
    "@types/react": "^17.0.2",
    "@types/rss": "^0.0.28",
    "@types/yup": "^0.29.11",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "gray-matter": "^4.0.2",
    "highlight.js": "^10.4.0",
    "marked": "^1.2.5",
    "next": "^10.0.2",
    "prettier": "^2.2.0",
    "pretty-quick": "^3.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rss": "^1.2.2",
    "serverless": "^2.23.0",
    "typescript": "^4.1.2"
  }
}
